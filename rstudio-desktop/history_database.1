1442430706613:top.correlated = function(X, subset=NULL, k=50) {
1442430706613:k = min(k, nrow(X) - 1)
1442430706614:X = as.matrix(X)
1442430706614:stopifnot(sum(is.na(X)) == 0)
1442430706614:Xs = t(apply(X,1,scale))
